{
  "name": "jira-field-formatter",
  "version": "3.0.0",
  "description": "A standalone Node.js library for formatting field values to be compatible with the Jira REST API",
  "main": "index.js",
  "type": "module",
  "scripts": {
  "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathIgnorePatterns=tests/live",
  "test:live": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=tests/live",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js",
    "example": "node examples/basic-usage.js",
    "release": "node scripts/release.js"
  },
  "keywords": [
    "jira",
    "api",
    "formatter",
    "field",
    "atlassian",
    "rest",
    "json",
    "submodule"
  ],
  "author": "FallingReign",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FallingReign/jira-field-formatter.git"
  },
  "bugs": {
    "url": "https://github.com/FallingReign/jira-field-formatter/issues"
  },
  "homepage": "https://github.com/FallingReign/jira-field-formatter#readme",
  "devDependencies": {
    "jest": "^29.7.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5"
  },
  "dependencies": {
    "dotenv": "^16.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
